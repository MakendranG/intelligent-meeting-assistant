{
  "name": "intelligent-meeting-assistant",
  "version": "1.0.0",
  "description": "AI-powered meeting assistant with real-time transcription, task management, and workflow integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node server.js",
    "start:dev": "nodemon server.js",
    "build:ts": "tsc",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:all": "node test-runner.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "serve": "node server.js",
    "check": "npx tsc --noEmit"
  },
  "keywords": [
    "meeting-assistant",
    "ai",
    "transcription",
    "task-management",
    "workflow-automation",
    "speech-recognition"
  ],
  "author": "Kiro Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/intelligent-meeting-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/intelligent-meeting-assistant/issues"
  },
  "homepage": "https://github.com/your-username/intelligent-meeting-assistant#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "rimraf": "^5.0.0",
    "http-server": "^14.1.0",
    "nodemon": "^3.0.0",
    "node-fetch": "^3.3.0"
  },
  "dependencies": {
    "ws": "^8.14.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^3.0.0",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.0",
    "node-cron": "^3.0.0",
    "openai": "^4.20.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/uuid": "^9.0.0",
    "@types/node-cron": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}